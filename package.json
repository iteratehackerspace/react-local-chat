{
  "name": "hyechat",
  "version": "0.0.1",
  "description": "hyechat",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-remarkable": "^1.1.1",
    "validator": "^6.1.0",
    "watson-developer-cloud": "^2.6.1",
    "ws": "^1.1.1",
    "xss": "^0.3.3"
  },
  "engines": {
    "node": ">=7.5.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator",
      "transform-runtime"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-3": "^6.11.0",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run front-end",
    "dev-server": "NODE_ENV='debug' webpack-dev-server",
    "front-end": "NODE_ENV='debug' webpack --progress --colors -d",
    "prod": "NODE_ENV='production' webpack --progress --colors -p",
    "lint": "eslint server.js lib/**"
  },
  "author": "Edgar Aroutiounian <edgar.factorial@gmail.com>",
  "license": "MIT"
}
